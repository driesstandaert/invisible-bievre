{
  "name": "invisible-bievre",
  "version": "0.0.1",
  "description": "AR/VR experience app",
  "homepage": "driesstandaert.com",
  "scripts": {
    "build": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "watch:externalServer": "webpack --mode=development --watch --externalServer",
    "bundle": "npm ci && npm run watch",
    "bundle:externalServer": "npm ci && npm run watch:externalServer",
    "production": "cross-env NODE_ENV=production webpack --mode=production",
    "lint-sass": "sass-lint -v -q --format=compact",
    "lint-js": "eslint --ext .js src/js/"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "template",
    "setup"
  ],
  "author": "Dries Standaert <dries.standaert@gmail.com> (http://www.driesstandaert.com)",
  "license": "MIT",
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "aframe": "^1.0.4",
    "autoprefixer": "^9.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "ajv": "^6.12.4",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "2.2.2",
    "clean-webpack-plugin": "^3.0.0",
